n = int(input())
negatives = []
zeros = []
positives = []
for _ in range(n):
    number = int(input())
    if number < 0:
        negatives.append(number)
    elif number == 0:
        zeros.append(number)
    else:
        positives.append(number)
for num in negatives:
    print(num)
for num in zeros:
    print(num)
for num in positives:
    print(num)